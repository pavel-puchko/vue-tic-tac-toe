{
  "hosting": {
    "public": "./dist",
    "redirects": [ {
      "source" : "/game/build.js",
      "destination" : "/build.js",
      "type" : 301
    },
    {
      "source" : "/game/favicon.png",
      "destination" : "/favicon.png",
      "type" : 301
    }],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
